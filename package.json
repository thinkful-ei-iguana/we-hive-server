    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "morgan": "^1.9.1",
    "pg": "^8.0.3",
    "treeize": "^2.1.2",
    "xss": "^1.0.6"
  },
  "devDependencies": {
